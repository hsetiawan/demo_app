<h1>Listing hendraposts</h1>

<table>
  <tr>
    <th>Content</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @hendraposts.each do |hendrapost| %>
  <tr>
    <td><%= hendrapost.content %></td>
    <td><%= hendrapost.user_id %></td>
    <td><%= link_to 'Show', hendrapost %></td>
    <td><%= link_to 'Edit', edit_hendrapost_path(hendrapost) %></td>
    <td><%= link_to 'Destroy', hendrapost, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Hendrapost', new_hendrapost_path %>
